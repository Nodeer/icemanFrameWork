<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="InfoBar">

        <!-- Text to display for title. -->
        <attr name="infobar_title_text" format="string" localization="suggested" />
        <!-- Text appearance to display for title. -->
        <attr name="infobar_title_text_appearance" format="integer" />
        <!-- Text to display for value. -->
        <attr name="infobar_value_text" format="string" localization="suggested" />
        <!-- Text appearance to display for value. -->
        <attr name="infobar_value_text_appearance" format="integer" />
        <!-- Hint text to display for value. -->
        <attr name="infobar_value_hint" format="string" localization="suggested" />
        <!-- Text color to display for value hint. -->
        <attr name="infobar_value_hint_color" format="integer" />
        <!-- Drawable to be drawn to the left of title. -->
        <attr name="infobar_drawable" format="reference" />
        <!-- The padding between the drawables and the title text. -->
        <attr name="infobar_drawable_padding" format="dimension" />
        <!-- Drawable to be drawn as arrow to the right of value. -->
        <attr name="infobar_arrow" format="reference" />
        <!-- The direction to be drawn for the arrow drawable. -->
        <attr name="arrow_direction">

            <!-- Drawable to be drawn to right direction. -->
            <enum name="right" value="0" />
            <!-- Drawable to be drawn to bottom direction. -->
            <enum name="bottom" value="1" />
        </attr>
        <!-- When true, show mandatory flag("*") with title text. -->
        <attr name="isMandatory" format="boolean" />
        <!-- When true, show arrow drawable. -->
        <attr name="hasArrow" format="boolean" />
        <!-- When true, show logo drawable. -->
        <attr name="hasDrawable" format="boolean" />
        <!-- The width of the drawable to be drawn. -->
        <attr name="infobar_icon_width" format="dimension" />
        <!-- The height of the drawable to be drawn. -->
        <attr name="infobar_icon_height" format="dimension" />
    </declare-styleable>
    <declare-styleable name="XsTitleView">

        <!-- Text to display for title. -->
        <attr name="title_text" format="string" localization="suggested" />
        <!-- Text appearance to display for title. -->
        <attr name="title_text_appearance" format="integer" />

        <!-- When true, this widget show left button. -->
        <attr name="title_show_left_btn" format="boolean" />
        <!-- When true, this widget show right button background. -->
        <attr name="title_show_right_btn_bg" format="boolean" />
        <!-- Text to display for left button. -->
        <attr name="title_btn_left_text" format="string" localization="suggested" />
        <!-- Drawable to be drawn on left button. -->
        <attr name="title_btn_left_drawable" format="reference" />
        <!-- Width of the keft button. -->
        <attr name="title_btn_left_width" format="dimension" />
        <!-- Height of the right button. -->
        <attr name="title_btn_left_height" format="dimension" />
        <!-- A drawable to use as left button's background. -->
        <attr name="title_btn_left_bg" format="reference" />

        <!-- Text to display for right button. -->
        <attr name="title_btn_text" format="string" localization="suggested" />
        <!-- Drawable to be drawn on right button. -->
        <attr name="title_btn_drawable" format="reference" />
        <!-- Text appearance to display for right button. -->
        <attr name="title_btn_text_appearance" format="integer" />
        <!-- Width of the right button. -->
        <attr name="title_btn_width" format="dimension" />
        <!-- Height of the right button. -->
        <attr name="title_btn_height" format="dimension" />
        <!-- A drawable to use as right button's background. -->
        <attr name="title_btn_bg" format="reference" />
        <!-- The padding between the drawables and the text for right button. -->
        <attr name="title_btn_text_padding" format="dimension" />
        <!-- When true, this widget use shadow drawable as its background. -->
        <attr name="title_bg_show_shadow" format="boolean" />
    </declare-styleable>
    <declare-styleable name="XsTextView">

        <!-- The drawable to be drawn. -->
        <attr name="text_drawable_src" format="reference" />
        <!-- The direction of the drawable to be drawn. -->
        <attr name="text_drawable_direction">

            <!-- The drawable to be drawn to the left of the text. -->
            <enum name="left" value="0" />
            <!-- The drawable to be drawn above the text. -->
            <enum name="top" value="1" />
            <!-- The drawable to be drawn to the right of the text. -->
            <enum name="right" value="2" />
            <!-- The drawable to be drawn below the text. -->
            <enum name="bottom" value="3" />
        </attr>
        <!-- The width of the drawable to be drawn. -->
        <attr name="text_drawable_width" format="dimension" />
        <!-- The height of the drawable to be drawn. -->
        <attr name="text_drawable_height" format="dimension" />
    </declare-styleable>
    <declare-styleable name="CtripLoadingLayout">

        <!-- loading layout id in widgt @layout/list_view_loading_indicator -->
        <attr name="loading_layout" format="reference" />
        <!-- When true, show error layout when server fail. -->
        <attr name="show_error_layout" format="boolean" />
        <!-- net unconnect layout id in widgt @layout/load_error_layout_net_unconnect_default -->
        <attr name="error_layout_net_unconnect" format="reference" />
        <!-- net fail layout id in widgt @layout/list_view_loading_indicator -->
        <attr name="error_layout_net_fail" format="reference" />
        <!-- net timeout layout id in widgt @layout/load_error_layout_net_timeout_default -->
        <attr name="error_layout_net_timeout" format="reference" />
        <!-- data fail layout id in widgt @layout/load_error_layout_net_data_fail_default -->
        <attr name="error_layout_data_fail" format="reference" />
        <!-- normal fail layout id in widgt @layout/load_error_layout_normal_default -->
        <attr name="error_layout_normal_fail" format="reference" />
        <!-- other fail layout id in widgt @layout/load_error_layout_normal_default -->
        <attr name="error_layout_other" format="reference" />
        <!-- no data layout id in widgt @layout/load_error_layout_nodata_default -->
        <attr name="error_layout_no_data_fail" format="reference" />
        <!-- no data layout id in widgt @layout/load_error_layout_nodata_filter_default -->
        <attr name="error_layout_filter_no_data_fail" format="reference" />
        <!-- no login layout id in widgt @layout/load_error_layout_nodata_filter_default -->
        <attr name="error_layout_no_login" format="reference" />
    </declare-styleable>
    <declare-styleable name="CtripTabGroupButton">

        <!-- tab size -->
        <attr name="tab_size" format="integer" />
    </declare-styleable>
    <declare-styleable name="CtripEditableInfoBar">

        <!-- Text to display for title. -->
        <attr name="editableinfo_title_value" format="string" />
        <!-- Hint text to display for edittext. -->
        <attr name="editableinfo_hint_value" format="string" />
        <!-- Text color to display for hint. -->
        <attr name="editableinfo_hint_color" format="integer" />
        <!-- When true, show mandatory flag("*") with title text. -->
        <attr name="editableinfo_is_necessary" format="boolean" />
        <!-- Text appearance to display for title. -->
        <attr name="editableinfo_title_appearance" format="integer" />
        <!-- Set an input filter to constrain the text length to the specified number. -->
        <attr name="editableinfo_edit_maxLength" format="integer" />
        <!-- Text appearance to display for edittext. -->
        <attr name="editableinfo_edit_appearance" format="integer" />
        <!-- Deprecated attribute. -->
        <attr name="editableinfo_need_clear" format="boolean" />
        <!-- When true, show arrow drawable. -->
        <attr name="editableinfo_need_arrow" format="boolean" />
        <!-- Drawable to be drawn to the left of title. -->
        <attr name="editableinfo_drawable" format="reference" />
        <!-- Drawable direction to the title -->
        <attr name="editableinfo_drawable_direction" format="integer" />
        <!-- The padding between the drawables and the title text. -->
        <attr name="editableinfo_drawable_padding" format="dimension" />
        <!-- The width of the drawable to be drawn. -->
        <attr name="editableinfo_drawable_width" format="dimension" />
        <!-- The height of the drawable to be drawn. -->
        <attr name="editableinfo_drawable_height" format="dimension" />
        <!-- The type of data being placed in a text field, used to help an input method decide how to let the user enter text. Generally you can select a single value, though some can be combined together as indicated. Setting this attribute to anything besides <var>none</var> also implies that the text is editable. -->
        <attr name="editableinfo_inputType">

            <!-- Just plain old text. Corresponds to {@link android.text.InputType#TYPE_CLASS_TEXT} | {@link android.text.InputType#TYPE_TEXT_VARIATION_NORMAL}. -->
            <flag name="text" value="0x00000001" />
            <!-- Can be combined with <var>text</var> and its variations to allow multiple lines of text in the field. If this flag is not set, the text field will be constrained to a single line. Corresponds to {@link android.text.InputType#TYPE_TEXT_FLAG_MULTI_LINE}. -->
            <flag name="textMultiLine" value="0x00020001" />
            <!-- Can be combined with <var>text</var> and its variations to allow multiple lines of text in the field. If this flag is not set, the text field will be constrained to a single line. Corresponds to {@link android.text.InputType#TYPE_TEXT_FLAG_MULTI_LINE}. -->
            <flag name="textEmailAddress" value="0x00020021" />
            <!-- Text that is a password. Corresponds to {@link android.text.InputType#TYPE_CLASS_TEXT} | {@link android.text.InputType#TYPE_TEXT_VARIATION_PASSWORD}. -->
            <flag name="textPassword" value="0x00000081" />
            <!-- A numeric only field. Corresponds to {@link android.text.InputType#TYPE_CLASS_NUMBER} | {@link android.text.InputType#TYPE_NUMBER_VARIATION_NORMAL}. -->
            <flag name="number" value="0x00000002" />
            <!-- A numeric password field. Corresponds to {@link android.text.InputType#TYPE_CLASS_NUMBER} | {@link android.text.InputType#TYPE_NUMBER_VARIATION_PASSWORD}. -->
            <flag name="numberPassword" value="0x00000012" />
            <!-- For entering a phone number. Corresponds to {@link android.text.InputType#TYPE_CLASS_PHONE}. -->
            <flag name="phone" value="0x00000003" />
        </attr>
    </declare-styleable>
    <declare-styleable name="CtripEditText">

        <!-- Hint text to display for edittext. -->
        <attr name="edit_hint_value" format="string" />
        <!-- Text color to display for hint. -->
        <attr name="edit_hint_color" format="integer" />
        <!-- Text appearance to display for edittext. -->
        <attr name="edit_appearance" format="integer" />
        <!-- Set an input filter to constrain the text length to the specified number. -->
        <attr name="edit_maxLength" format="integer" />
        <!-- A drawable to use as the background. This can be either a reference to a full drawable resource (such as a PNG image, 9-patch, XML state list description, etc), or a solid color such as "#ff000000" (black). -->
        <attr name="edit_background" format="integer" />
        <!-- The type of data being placed in a text field, used to help an input method decide how to let the user enter text. Generally you can select a single value, though some can be combined together as indicated. Setting this attribute to anything besides <var>none</var> also implies that the text is editable. -->
        <attr name="edit_inputType">

            <!-- Just plain old text. Corresponds to {@link android.text.InputType#TYPE_CLASS_TEXT} | {@link android.text.InputType#TYPE_TEXT_VARIATION_NORMAL}. -->
            <flag name="text" value="0x00000001" />
            <!-- Can be combined with <var>text</var> and its variations to allow multiple lines of text in the field. If this flag is not set, the text field will be constrained to a single line. Corresponds to {@link android.text.InputType#TYPE_TEXT_FLAG_MULTI_LINE}. -->
            <flag name="textMultiLine" value="0x00020001" />
            <!-- Can be combined with <var>text</var> and its variations to allow multiple lines of text in the field. If this flag is not set, the text field will be constrained to a single line. Corresponds to {@link android.text.InputType#TYPE_TEXT_FLAG_MULTI_LINE}. -->
            <flag name="textEmailAddress" value="0x00020021" />
            <!-- Text that is a password. Corresponds to {@link android.text.InputType#TYPE_CLASS_TEXT} | {@link android.text.InputType#TYPE_TEXT_VARIATION_PASSWORD}. -->
            <flag name="textPassword" value="0x00000081" />
            <!-- A numeric only field. Corresponds to {@link android.text.InputType#TYPE_CLASS_NUMBER} | {@link android.text.InputType#TYPE_NUMBER_VARIATION_NORMAL}. -->
            <flag name="number" value="0x00000002" />
            <!-- A numeric password field. Corresponds to {@link android.text.InputType#TYPE_CLASS_NUMBER} | {@link android.text.InputType#TYPE_NUMBER_VARIATION_PASSWORD}. -->
            <flag name="numberPassword" value="0x00000012" />
            <!-- For entering a phone number. Corresponds to {@link android.text.InputType#TYPE_CLASS_PHONE}. -->
            <flag name="phone" value="0x00000003" />
        </attr>
    </declare-styleable>

</resources>